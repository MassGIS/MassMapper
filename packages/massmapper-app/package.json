	"scripts": {
		"build:dev": "npm run clean && NODE_ENV=development node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js --progress --config webpack.config.js",
		"build:dev:watch": "npm run clean && NODE_ENV=development node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js --progress --config webpack.config.js --watch",
