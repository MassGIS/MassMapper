{
	"name": "@massgis/oliver-app",
	"version": "1.0.0",
	"description": "OLIVER App",
	"scripts": {
		"build:dev": "npm run clean && NODE_ENV=development --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js --progress --config webpack.config.js",
		"build:dev:watch": "npm run clean && NODE_ENV=development node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js --progress --config webpack.config.js --watch",
		"build:prod": "NODE_ENV=production node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js --progress --config webpack.config.js --display-used-exports",
		"build:prod:watch": "NODE_ENV=production node --max_old_space_size=8192 ../../node_modules/webpack/bin/webpack.js --progress --config webpack.config.js --watch --display-used-exports",
		"clean": "rm -f ../../dist/oliverApp*"
	},
	"devDependencies": {
		"@types/detect-browser": "^3.0.0",
		"@types/isomorphic-fetch": "0.0.34",
		"@types/query-string": "~6.1.0",
		"@types/react": "^17.0.3",
		"@types/react-css-modules": "^4.6.2",
		"@types/react-dom": "^17.0.3",
		"@types/react-router-dom": "^5.1.7",
		"@types/uuid": "^8.3.0",
		"webpack": "^4.42.1"
	},
	"dependencies": {
		"@turf/turf": "^6.3.0",
		"date-fns": "^2.16.1",
		"detect-browser": "^4.0.3",
		"fetch-retry": "^4.0.1",
		"isomorphic-fetch": "^2.2.1",
		"mobx": "^6.1.8",
		"mobx-react": "^7.1.0",
		"mobx-react-lite": "^3.2.0",
		"query-string": "^5.1.0",
		"react": "^17.0.2",
		"react-css-modules": "^4.7.11",
		"react-dom": "^17.0.2",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"typedi": "^0.10.0",
		"uuid": "^8.3.2"
	}
}
